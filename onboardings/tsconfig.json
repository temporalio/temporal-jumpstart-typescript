{
  "extends": "@tsconfig/node21/tsconfig.json",
  "version": "4.4.2",
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "outDir": "./build",
    "esModuleInterop": true,
    "paths": {
        "@generated/*": ["./generated/*"]
      }
  },
  "include": [
    "eslint.config.js",
    "scripts/**/*.ts",
    "app/src/**/*.ts",
    "onboardings/src/**/*.ts",
  ]
}