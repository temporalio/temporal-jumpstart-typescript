{
  "name": "temporal-jumpstart-typescript",
  "version": "1.0.0",
  "description": "Temporal Jumpstart for TypeScript",
  "directories": {
    "doc": "docs"
  },
  "type": "module",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "app": "tsx app/src/index.ts",
    "app.watch": "tsx watch app/src/index.ts",
    "onboardings:build": "tsc --build onboardings/scripts/build-workflow-bundle.ts",
    "onboardings": "tsx onboardings/src/index.ts",
    "onboardings.watch": "tsx watch onboardings/src/index.ts",
    "onboardings:api": "tsx onboardings/src/api/index.ts",
    "update": "npm-check-updates -u && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/temporalio/temporal-jumpstart-typescript.git"
  },
  "keywords": [
    "TypeScript",
    "Temporal",
    "Jumpstart"
  ],
  "author": "Mike Nichols <mike.nichols@temporal.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/temporalio/temporal-jumpstart-typescript/issues"
  },
  "homepage": "https://github.com/temporalio/temporal-jumpstart-typescript#readme",
  "dependencies": {
    "@temporalio/activity": "^1.11.3",
    "@temporalio/client": "^1.11.3",
    "@temporalio/worker": "^1.11.3",
    "@temporalio/workflow": "^1.11.3",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.9.0",
    "express": "^4.21.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.46.0",
    "@bufbuild/protobuf": "^2.2.2",
