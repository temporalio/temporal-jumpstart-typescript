    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@stylistic/eslint-plugin-js": "^2.10.1",
    "@stylistic/eslint-plugin-ts": "^2.10.1",
    "@tsconfig/node21": "^21.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/ms": "^0.7.34",
