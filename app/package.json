{
  "name": "jumpstart-app",
  "version": "1.0.0",
  "description": "Temporal Jumpstart for TypeScript",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean:fixtures": "shx rm -rf fixtures/generated && shx mkdir -p fixtures/generated/clients/temporal",
    "build:fixtures": "echo 'no fixtures used'",
    "pretest": "npm run build:fixtures",
    "test": "mocha 'src/**/*.spec.*'",
    "build:clean": "shx rm -rf build",
    "build": "tsc --build",
    "build:workflow": "tsx ./src/scripts/build-workflow-bundle.ts",
    "build.watch": "tsc --build --watch",
    "domain": "run-s build:clean build build:workflow && shx cp .env build && NODE_ENV=production node build/index.js",
    "domain:local": "tsc --noEmit && tsx ./src/index.ts",
    "domain.watch": "tsx watch ./src/index.ts",
    "api": "tsc --noEmit && tsx ./src/api/index.ts",
    "update": "npm-check-updates -u && npm i",
    "downloadtestserver:windows": "PLATFORM=windows ARCH=amd64 DEST=bin tsx src/scripts/fetch-test-server.ts",
    "downloadtestserver:mac": "PLATFORM=darwin DST=bin tsx src/scripts/fetch-test-server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/temporalio/temporal-jumpstart-typescript.git"
  },
  "keywords": [
    "TypeScript",
    "Temporal",
    "Jumpstart"
  ],
  "author": "Mike Nichols <mike.nichols@temporal.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/temporalio/temporal-jumpstart-typescript/issues"
  },
  "homepage": "https://github.com/temporalio/temporal-jumpstart-typescript#readme",
  "dependencies": {
    "@temporalio/activity": "^1.11.5",
    "@temporalio/client": "^1.11.5",
    "@temporalio/worker": "^1.11.5",
    "@temporalio/workflow": "^1.11.5",
    "cors": "^2.8.5",
    "dotenv-extended": "^2.9.0",
    "express": "^4.21.1",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@stylistic/eslint-plugin-js": "^2.11.0",
    "@stylistic/eslint-plugin-ts": "^2.11.0",
    "@temporalio/testing": "^1.11.5",
    "@tsconfig/node21": "^21.0.3",
    "@types/chai": "^5.0.1",
    "@types/chai-as-promised": "^8.0.1",
    "@types/cors": "^2.8.17",
    "@types/download": "^8.0.5",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.10",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.9.3",
    "@types/sinon": "^17.0.3",
    "@types/stack-utils": "^2.0.3",
    "@types/supports-color": "^8.1.3",
    "@typescript-eslint/parser": "^8.15.0",
    "arg": "^5.0.2",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.1",
    "download": "^8.0.0",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.2.0",
    "globals": "^15.12.0",
    "inspector": "^0.5.0",
    "mkdirp": "^3.0.1",
    "mocha": "^10.8.2",
    "npm-check-updates": "*",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  }
}
